{
  "name": "health-tracker",
  "version": "0.1.3",
  "description": "Frontend webapp for the Rainbow Devs Health Tracker application.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rainbow-Devs/health-tracker.git"
  },
  "homepage": "https://github.com/Rainbow-Devs/health-tracker",
  "author": {
    "name": "Rainbow Devs",
    "url": "https://github.com/Rainbow-Devs"
  },
  "contributors": [],
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "^18.16.0 || >=20.2.0"
  },
  "keywords": [],
  "packageManager": "pnpm@8.5.1",
  "type": "module",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && pnpm lint"
    }
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "pnpm eslint:check && pnpm alex:check",
    "eslint:check": "next lint",
    "eslint:fix": "next lint --fix",
    "alex:check": "alex .",
    "format": "pnpm prettier:overwrite",
    "prettier:check": "prettier --check ./src",
    "prettier:overwrite": "prettier --write ./src",
    "clean": "rm -rf .next node_modules pnpm-lock.yaml package-lock.json yarn.lock .pnpm-store"
  },
  "dependencies": {
    "next": "^13.4.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.8",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "alex": "^11.0.0",
    "autoprefixer": "^10.4.14",
    "class-variance-authority": "^0.6.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.42.0",
    "eslint-config-next": "^13.4.4",
    "eslint-plugin-github": "^4.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tailwindcss": "^3.12.1",
    "husky": "^8.0.3",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.1.3"
  }
}
